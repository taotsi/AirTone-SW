   1670            assert_param(IS_TIM2_TIXCLK_SOURCE(TIM2_TIxExternalCLKSource));
   1671            assert_param(IS_TIM2_IC_POLARITY(TIM2_ICPolarity));
   1672            assert_param(IS_TIM2_IC_FILTER(ICFilter));
